@model IEnumerable<EMarket.Areas.Admin.Models.NhaCungCap>
@{
    Layout = null;
}
<div class="aside">
    <h3 class="aside-title">Nhà Cung Cấp</h3>
    <div class="checkbox-filter">
        @{int i = 1;}
        @foreach (var item in Model)
        {
            <div class="input-checkbox">
                <input type="checkbox" id="brand-@i">
                <label for="brand-@i">
                    <span></span>
                    @item.TenNhaCungCap
                </label>
            </div>
            i++;
        }
    </div>
</div>
