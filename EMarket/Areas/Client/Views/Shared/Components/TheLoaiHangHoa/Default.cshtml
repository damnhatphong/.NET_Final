@model IEnumerable<EMarket.Areas.Admin.Models.Loai>
@{
    Layout = null;
    int i = 1;
}

    <!-- aside Widget -->
 <div class="aside">
    <h3 class="aside-title">Loại Hàng Hóa</h3>
    <div class="checkbox-filter">
        @foreach (var x in Model) { 
            <div class="input-checkbox">
                <input type="checkbox" id="category-@i">
                <label for="category-@i">
                    <span></span>
                    @x.TenLoai
                </label>
            </div>
            i++;
        }
    </div>
</div>
