@model IEnumerable<EMarket.Areas.Admin.Models.TaiKhoan>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NgayDk)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoaiTaiKhoan)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>           
            <td>
                @Html.DisplayFor(modelItem => item.NgayDk)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoaiTaiKhoan)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TaiKhoanId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TaiKhoanId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TaiKhoanId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
